<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
        </tr>
    </thead>

    {{#each archivoJson}}
    <tbody>
        <tr>
            <td>{{this.nombre}}</td>
            <td>${{this.precio}}</td>
        </tr>
    </tbody>
    {{/each}}
</table>